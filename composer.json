{
    "name": "craftremote/plugin",
    "description": "Remotely manage your Craft CMS installs.",
    "type": "craft-plugin",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "craftremote"
    ],
    "support": {
        "docs": "https://github.com/craftremote/plugin/blob/master/README.md",
        "issues": "https://github.com/craftremote/plugin/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Rias",
            "homepage": "https://rias.be"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-RC1"
    },
    "autoload": {
        "psr-4": {
          "craftremote\\plugin\\": "src/"
        }
    },
    "extra": {
        "name": "craftremote",
        "handle": "craftremote",
        "hasCpSettings": true,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/craftremote/plugin/master/CHANGELOG.md",
        "components": {
            "craftremoteService": "craftremote\\plugin\\services\\CraftremoteService"
        },
        "class": "craftremote\\plugin\\Craftremote"
    }
}
